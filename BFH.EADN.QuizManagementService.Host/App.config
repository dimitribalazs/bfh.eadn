<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <services>
            <service behaviorConfiguration="WcfServiceBehavior" name="BFH.EADN.QuizManagementService.Implementation.QuizManagement">
                <endpoint address="http://localhost:5001/QuizManagement" binding="basicHttpBinding"
                    bindingConfiguration="" contract="BFH.EADN.QuizManagementService.Contracts.IQuizManagement" />
              <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
            </service>
        </services>
      <behaviors>
        <serviceBehaviors>
          <behavior name="WcfServiceBehavior" >
            <!-- Add the following element to your service behavior configuration. -->
            <serviceMetadata httpGetEnabled="true" />
          </behavior>
        </serviceBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>